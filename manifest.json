{
  "manifest_version": 2,
  "name": "Crosh Window",
  "description": "Start a Crosh Window (uses Secure Shell)",
  "version": "1.1",
  "offline_enabled": true,
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  }
}
